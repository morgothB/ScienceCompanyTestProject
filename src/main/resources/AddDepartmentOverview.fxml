<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="150.0" maxWidth="300.0" minHeight="150.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.depatmentControlWindow.AddDepartmentController">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <bottom>
                <ButtonBar BorderPane.alignment="CENTER">
                    <buttons>
                        <Button mnemonicParsing="false" text="Add" />
                        <Button layoutX="225.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
                    </buttons>
                    <BorderPane.margin>
                        <Insets bottom="10.0" right="10.0" />
                    </BorderPane.margin>
                </ButtonBar>
            </bottom>
            <center>
                <TextField fx:id="textName" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets bottom="30.0" left="10.0" right="10.0" />
                    </BorderPane.margin>
                </TextField>
            </center>
            <top>
                <Label text="Name" textAlignment="CENTER" BorderPane.alignment="CENTER">
                    <font>
                        <Font name="Arial Bold" size="24.0" />
                    </font>
                    <padding>
                        <Insets top="30.0" />
                    </padding>
                </Label>
            </top></BorderPane>
    </children>
</AnchorPane>
